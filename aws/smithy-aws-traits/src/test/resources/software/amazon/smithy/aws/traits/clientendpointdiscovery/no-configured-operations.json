                }
            },
            "DescribeEndpointsOutput": {
                "type": "structure",
                "members": {
                    "Endpoints": {
                        "target": "Endpoints"
                    }
                }
            },
            "Endpoints": {
                "type": "list",
                "member": {
                    "target": "Endpoint"
                }
            },
            "Endpoint": {
                "type": "structure",
                "members": {
                    "Address": {
                        "target": "smithy.api#String"
                    },
                    "CachePeriodInMinutes": {
                        "target": "smithy.api#Long"
                    }
                }
            },
